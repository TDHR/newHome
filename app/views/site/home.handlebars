<div class="page-home">
  <div class="banner-home hidden-xs"></div>
  <div class="block-1">
    <div class="container">
      <div class="row">
        <div class="col-md-5">
          <div class="home-character-1">
            <img src="/images/home-character-1.jpg" />
          </div>
        </div>
        <div class="col-md-7">
          <div class="title">
            <span class="underline">{{__ 'home-block1-title1'}}</span>
          </div>
          <div class="content">
            <p>{{__ 'home-block1-content1'}}</p>
