              <li>
                <div class="number">03</div>
                <div class="article">
                  <div class="title">{{__ 'home-block5-article3-title'}}</div>
                  <div class="content">
                    <p>{{__ 'home-block5-article3-content1'}}</p>
                    <p>{{__ 'home-block5-article3-content2'}}</p>
                  </div>
                </div>
