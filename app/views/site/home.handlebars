          <div class="content">
            <p>{{__ 'home-block2-card3-content1'}}</p>
            <p>{{__ 'home-block2-card3-content2'}}</p>
          </div>
        </a>
      </div>
    </div>
  </div>
  <div class="block-3">
    <div class="container">
      <div class="block-header">
        <a href="/service">
          <span>{{__ 'home-blcok3-title'}}</span>
