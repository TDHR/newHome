<body class="page-help-intro">
  <div class="language-menu">
    <a id="languageBtn" data-language="{{__ 'language-code'}}">
      <img src="/images/{{__ 'language-code'}}.png">{{__ 'language'}}
    </a>
  </div>
  <div class="reveal">
    <div class="slides">
      <section>
        <h2 class="theme-color">{{__ 'help-intro-main-title'}}</h2>
        <p class="theme-color">© 2016 REITs (Beijing) FinTech Co., Ltd. All Rights Reserved</p>
      </section>

      <section data-transition="zoom-in fade-out">
        <h2 class="theme-color">{{__ 'help-intro-issue-assets'}}</h2>
      </section>
      
      <section data-transition="convex">
        <div class="assets-holder">
          <img id="picAsset1" class="intro-pic intro-pic-asset1" src="/images/intro-pic-asset1.jpg">
          <img id="picAsset2" class="intro-pic intro-pic-asset2" src="/images/intro-pic-asset2.jpg">
        </div>
        <div class="text">1. {{__ 'help-intro-issue-assets-1'}}</div>
      </section>

      <section data-transition="convex">
        <div class="icon-holder">
          <img id="iconSurvey" class="intro-icon" src="/images/intro-icon-survey.png">
        </div>
        <div class="text">2. {{__ 'help-intro-issue-assets-2'}}</div>
      </section>

      <section data-transition="convex">
        <div class="icon-holder">
          <img id="iconCheck" class="intro-icon" src="/images/intro-icon-check.png">
        </div>
        <div class="text">3. {{__ 'help-intro-issue-assets-3'}}</div>
      </section>

      <section data-transition="convex">
        <div class="icon-holder">
          <img id="iconFinance" class="intro-icon" src="/images/intro-icon-finance.png">
        </div>
        <div class="text">4. {{__ 'help-intro-issue-assets-4'}}</div>
      </section>

      <section data-transition="zoom-in fade-out">
        <h2 class="theme-color">{{__ 'help-intro-fund-raising'}}</h2>
      </section>
      
      <section data-transition="convex">
        <div class="icon-holder">
          <img id="iconChose" class="intro-icon" src="/images/intro-icon-chose.png">
        </div>
        <div class="text">1. {{__ 'help-intro-fund-raising-1'}}</div>
      </section>

      <section data-transition="convex">
        <div class="icon-holder">
          <div id="iconProgress">
            <span class="percentage">0%</span>
            <span class="bar"></span>
          </div>
        </div>
        <div class="text">2. {{__ 'help-intro-fund-raising-2'}}</div>
      </section>
      
      <section data-transition="convex">
        <div class="icon-holder">
          <img id="iconWallet" class="intro-icon" src="/images/intro-icon-wallet.png">
          <img id="iconWalletCoin" class="icon-wallet-coin intro-coin" src="/images/intro-icon-rts.png">
        </div>
        <div class="text">3. {{__ 'help-intro-fund-raising-3'}}</div>
      </section>

      <section data-transition="zoom-in fade-out">
        <h2 class="theme-color">{{__ 'help-intro-income'}}</h2>
      </section>
      
      <section data-transition="convex">
        <div class="pic-holder">
          <img id="picTrade" class="intro-pic" src="/images/intro-trade.png">
        </div>
        <div class="text">1. {{__ 'help-intro-income-1'}}</div>
      </section>

      <section data-transition="convex">
        <div class="pic-holder">
          <img id="iconCatcher" class="icon-catcher" src="/images/intro-icon-catcher.png">
          <img class="icon-catcher-coin icon-catcher-coin1" src="/images/intro-icon-btc.png">
          <img class="icon-catcher-coin icon-catcher-coin2" src="/images/intro-icon-btc.png">
          <img class="icon-catcher-coin icon-catcher-coin3" src="/images/intro-icon-btc.png">
          <img class="icon-catcher-coin icon-catcher-coin4" src="/images/intro-icon-btc.png">
          <img class="icon-catcher-coin icon-catcher-coin5" src="/images/intro-icon-btc.png">
          <img class="icon-catcher-coin icon-catcher-coin6" src="/images/intro-icon-btc.png">
          <img class="icon-catcher-coin icon-catcher-coin7" src="/images/intro-icon-btc.png">
        </div>
        <div class="text">2. {{__ 'help-intro-income-2'}}</div>
      </section>

      <section data-transition="convex">
        <div class="pic-holder repurchase-holder">
          <img id="picRts" class="intro-pic" src="/images/intro-icon-rts.png">
          <img id="picRmb" class="intro-pic" src="/images/intro-icon-rmb.png">
        </div>
        <div class="text">3. {{__ 'help-intro-income-3'}}</div>
      </section>
      
      <section data-transition="zoom-in fade-out">
        <h3>{{__ 'help-intro-thanks'}}</h3>
        <div class="contact">
          <div class="wechat">
            <img src="/images/wechat.png">
          </div>
          <div class="website">
            <div class="company">{{__ 'help-intro-company'}}</div>
            <div>
              {{__ 'help-intro-website'}}
              <a href="https://reitschain.com">https://reitschain.com</a>
            </div>
            <div>
              {{__ 'help-intro-tel'}}
              <a href="tel:010-89943073">010-89943073</a>
            </div>
            <div>
              {{__ 'help-intro-email'}}
              <a href="mailTo:bd@reitsfax.com">bd@reitsfax.com</a>
            </div>
          </div>
        </div>
      </section>
      
    </div>
  </div>
  <script src="/libs/jquery.min.js"></script>
  <script src="/libs/reveal/js/reveal.js"></script>
  <script src="/scripts/help-intro.js{{fileTail}}"></script>
