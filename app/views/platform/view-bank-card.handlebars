<div class="page-view-bank-card">
  <div class="container">
    <div class="row">
      <div class="col-md-3 col-sm-3">
        {{> sidebar}}
      </div>
      <div class="col-md-9 col-sm-9">
        <ol class="breadcrumb platform-breadcrumb">
          <li><a href="/user/security">{{__ 'sidebar-security'}}</a></li>
          <li class="active">{{__ "view-bank-card-title"}}</li>
        </ol>
        <div class="list">
          <p>{{__ 'view-bank-name'}}{{bank.bankName}}</p>
          <p>{{__ 'view-bank-card-number'}}{{handleText bank.bankCardNum '5'}}</p>
          <p>{{__ 'view-bank-card-realname'}}{{handleText bank.name '2'}}</p>
          <p>{{__ 'view-bank-card-id'}}{{handleText bank.idNumber '3'}}</p>
          <p>{{__ 'view-bank-card-time'}}{{dateFormat bank.verifTime 'default'}}</p>
        </div>
      </div>
    </div>
  </div>
</div>

{{> footer}}
{{> scripts}}
